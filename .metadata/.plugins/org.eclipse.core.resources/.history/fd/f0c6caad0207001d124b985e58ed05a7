package com.ssafy.ws.step3;

import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {
		int N;
		int T;
		Scanner sc = new Scanner(System.in);
		T = sc.nextInt();
		for (int t = 1; t <= T; t++) {
			N = sc.nextInt();
			char[][] arr = new char[50][50];
			int[][] height = new int [50][50];
			int[] dir_y = {1, 1, 0, -1, -1, -1, 0, 1};
			int[] dir_x = {0, 1, 1, 1, 0, -1, -1, -1};
			//입력
			for (int y = 0; y < N; y++) {
				for (int x= 0; x < N; x++) {
					arr[y][x] = sc.next().charAt(0);
				}
			}
			//탐색
			int Bcnt = 0;
			for (int y = 0; y < N; y++) {
				for (int x= 0; x < N; x++) {
					Bcnt = 0;
					for (int i = 0; i < 8; i++) {
						int new_y = y + dir_y[i];
						int new_x = x + dir_x[i];
						if ((new_x >= 0 && new_x < N) && (new_y >= 0 && new_y < N)) {
							if (arr[new_y][new_x] == 'G')
							{
								height[new_y][new_x] = 2;
							}
							else {
								Bcnt++;
								if (Bcnt == 8) {
									//가로
									for (int wid = 0; wid < N; wid++) {
										if (arr[new_y][wid] == 'B')
											height[new_y][new_x]++;
									}
									//세로
									for (int hei = 0; hei < N; hei++) {
										if (arr[hei][new_x] == 'B')
											height[new_y][new_x]++;
									}
									if (arr[new_y][new_x] == 'B')
										height[new_y][new_x]--;
								}
							}
						}
					}
				}
			}
			//가장 큰 값 찾기
			int max = 0;
			for (int y = 0; y < N; y++) {
				for (int x = 0; x< N; x++) {
					if (max < height[y][x])
						max = height[y][x];
				}
			}
			System.out.println("#"+t+" " + max);
			
		}
		sc.close();
	}
}
